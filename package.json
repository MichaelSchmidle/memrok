{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev --host",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "setup": "bun run certs && bun run infra:start",
    "certs": "cd deployment && ./scripts/generate-certs.sh",
    "infra:start": "docker compose --env-file .env -f deployment/docker-compose.yml -f deployment/docker-compose.dev.yml down && docker compose --env-file .env -f deployment/docker-compose.yml -f deployment/docker-compose.dev.yml up -d",
    "infra:restart": "docker compose --env-file .env -f deployment/docker-compose.yml -f deployment/docker-compose.dev.yml restart",
    "infra:stop": "docker compose --env-file .env -f deployment/docker-compose.yml -f deployment/docker-compose.dev.yml down",
    "infra:logs": "docker compose --env-file .env -f deployment/docker-compose.yml -f deployment/docker-compose.dev.yml logs -f",
    "infra:status": "docker compose --env-file .env -f deployment/docker-compose.yml -f deployment/docker-compose.dev.yml ps"
  },
  "dependencies": {
    "@formkit/auto-animate": "^0.8.2",
    "@formkit/tempo": "^0.1.2",
    "@nuxt/fonts": "0.11.4",
    "@nuxt/icon": "1.15.0",
    "@nuxt/image": "^1.10.0",
    "@nuxt/ui-pro": "^3.2.0",
    "@nuxtjs/i18n": "^9.5.6",
    "nuxt": "^3.17.6",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@iconify-json/ph": "^1.2.2"
  }
}